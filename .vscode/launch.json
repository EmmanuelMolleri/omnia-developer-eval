{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "auth-api",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/auth-api/bin/Debug/net8.0/auth-api.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/auth-api",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/src/auth-api/Views"
      }
    },
